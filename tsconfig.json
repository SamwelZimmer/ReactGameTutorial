{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/RecoilRootWrapper.ts", "components/level-layout/LevelBackgroundTilesLayer.jsx", "components/level-layout/LevelPlacementsLayer.jsx", "components/level-layout/MapCell.jsx", "components/level-layout/RenderLevel.jsx", "game-objects/CelebrationPlacement.jsx", "game-objects/FlourPlacement.jsx", "game-objects/GoalPlacement.jsx", "game-objects/HeroPlacement.jsx", "game-objects/LockPlacements.jsx", "game-objects/WallPlacement.jsx", "game-objects/Placement.jsx", "game-objects/KeyPlacements.jsx", "classes/Inventory.js", "classes/GameLoop.js", "classes/DirectionControls.js", "classes/Collision.js", "components/hud/LevelCompleteMessage.jsx", "components/hud/FlourCount.jsx", "classes/PlacementFactory.js", "classes/LevelState.js"],
  "exclude": ["node_modules"]
}
